          "slice": 0
        },
        "material": {
          "css": ".s-price-instructions-style .a-text-bold",
          "type": "Text",
          "slice": 0
        },
        "price_discounted": {
          "css": ".a-price[data-a-size=mini][data-a-strike=true], .a-price[data-a-size=\"s\"][data-a-strike=true], .a-price[data-a-size=\"b\"][data-a-strike=true]",
          "type": "number",
          "slice": 8
        },
        "current_price": {
          "css": ".s-underline-text.s-underline-link-text > span.a-price > span.a-offscreen:first-child",
          "type": "number",
          "slice": 0
        },
        "thereIsDiscount": {
          "css": "",
